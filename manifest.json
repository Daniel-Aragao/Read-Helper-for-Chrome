{
  "manifest_version": 2,
  "name": "Read helper",
  "version": "1.0.0.0",
  "version_name":"Blackground",

  "description": "This extension change the website's colors to make it easy to read when triggered. Also allows you to change colors on your own.",
  "icons": { "16": "images/favicon.png",
           "48": "images/extensionsManagementPage.png",
          "128": "images/webStoreIcon.png" },

  "browser_action": {
    "default_icon": {"19":"images/icon.png"},
    "default_popup": "Html/popup.html",
    "default_title": "Read mode"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}